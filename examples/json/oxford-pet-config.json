{
    "dataset_params": {
        "class_name": "HDF5Reader",
        "config": {
            "filename": "../../oxford_pet_data_new.h5",
            "batch_size": 64,
            "x_name": "x",
            "y_name": "y",
            "batch_cache": 4,
            "train_folds": [
                0,
                1,
                2,
                3,
                4
            ],
            "val_folds": [
                9
            ],
            "test_folds": [
                11,
                12,
                13
            ],
            "preprocessors": []
        }
    },
    "train_params": {
        "epochs": 5
    },
    "input_params": {
